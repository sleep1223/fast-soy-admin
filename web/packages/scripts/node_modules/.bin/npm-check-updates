#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/src/bin/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/src/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/src/bin/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/src/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../npm-check-updates/build/src/bin/cli.js" "$@"
else
  exec node  "$basedir/../npm-check-updates/build/src/bin/cli.js" "$@"
fi
