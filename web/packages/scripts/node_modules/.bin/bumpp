#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/bumpp@9.4.1/node_modules/bumpp/bin/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/bumpp@9.4.1/node_modules/bumpp/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/bumpp@9.4.1/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/bumpp@9.4.1/node_modules/bumpp/bin/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/bumpp@9.4.1/node_modules/bumpp/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/bumpp@9.4.1/node_modules:/mnt/d/Project/admin/soybean-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../bumpp/bin/bumpp.js" "$@"
else
  exec node  "$basedir/../bumpp/bin/bumpp.js" "$@"
fi
